{
  "name": "nodejs-app",
  "version": "1.0.0",
  "description": "NodeJS, TS Playground",
  "main": "index.js",
  "scripts": {
    "pretest": "",
    "test": "tsc -v",
    "posttest": "node -e console.log('DoneTesting')",
    "build-all": "tsc --build",
    "build-all-clean": "tsc --build --clean",
    "build-server": "tsc -p server",
    "check": "eslint server.js",
    "build-client": "tsc -p client",
    "serve-server": "node ./out/server/server.js",
    "serve-client": "node ./out/client/client.js",
    "serve-client2": "node ./out/client/test.js",
    "debug": "node --inspect=9229 ./out/server/server.js",
    "lint": "eslint server/*ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asatklichov/nodejs-app.git"
