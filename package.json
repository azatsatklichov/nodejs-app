  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.25.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "assert": "^2.0.0",
